{
  "name": "pet-angular",
  "version": "0.0.0",
  "lint-staged": {
    "*.{ts,html,scss,json,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
